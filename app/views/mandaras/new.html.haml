              = f.text_area :subtext7, class: 'subinput-text',rows: "3", wrap: "hard"
            %td#submandara1.table1-8
              = f.text_area :subtext8, class: 'subinput-text',rows: "3", wrap: "hard"
            %td#submandara1.table1-9
              = f.text_area :subtext9, class: 'subinput-text',rows: "3", wrap: "hard"
            %td.table6-7
            %td.table6-8
            %td.table6-9

      %table
        %tbody
          %tr
            %td.table7-1
            %td.table7-2
            %td.table7-3
            %td.table8-1
            %td.table8-2
            %td.table8-3
            %td.table9-1
