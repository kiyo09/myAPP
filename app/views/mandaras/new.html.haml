              = f.text_area :subtext2, class: 'subinput-text',rows: "3", wrap: "hard"
            %td#submandara1.table1-3
              = f.text_area :subtext3, class: 'subinput-text',rows: "3", wrap: "hard"
            %td.table6-1
            %td.table6-2
            %td.table6-3
          %tr
            %td.table5-4
            %td
            %td.table5-6
            %td#submandara1.table1-4
              = f.text_area :subtext4, class: 'subinput-text',rows: "3", wrap: "hard"
            %td#mandara1.table1-1
              = f.text_area :text, class: 'input-text',rows: "3", wrap: "hard"
            %td#submandara1.table1-6
              = f.text_area :subtext6, class: 'subinput-text',rows: "3", wrap: "hard"
            %td.table6-4
