.wrapper  
  = render "layouts/sidebar"

  .mand
    = form_for [@mandara] do |f|
      %table
        %tbody
          %tr
            %td.table2-1
            %td.table2-2
            %td.table2-3
            %td.table3-1
            %td.table3-2
            %td.table3-3
            %td.table4-1
