.wrapper  
  = render "layouts/sidebar"

  .contents
    .reports
      .report
        - @posts.each do |post|
          .repo
            = link_to "日付", user_path(current_user), class: 'repo'
            %br
            = link_to "#{post.updated_at.strftime("%m月%d日")}", post_path(post), class: 'repo'
            %br
            = link_to "TITLE", user_path(current_user), class: 'repo'
            = link_to "#{post.text}", user_path(current_user), class: 'repo'
            %br 
       
